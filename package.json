{
  "name": "playground",
  "version": "2.0.0",
  "description": "Sandbox for testing and experimenting",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "server": "node server/index.js 1000",
    "build": "webpack --config config/webpack.prod.js",
    "format": "prettier --write \"src/**/*.js\"",
    "eslint": "eslint --ext .js src/ -f html -o coverage/eslint-report.html",
    "jest-reset": "jest --clearCache",
    "e2e": "cypress run",
    "e": "cypress open",
    "test": "jest --env=jsdom --coverage --json --outputFile=./coverage/outputfile.json",
    "t": "jest --env=jsdom clipboardActions.test.js --watch",
    "w": "jest --env=jsdom --watchAll=true"
  },
  "keywords": [],
  "author": "CJV",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "27.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "cypress": "8.3.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "27.1.0",
    "prettier": "2.3.2",
    "react-test-renderer": "17.0.2",
    "redux-logger": "^3.0.6",
    "style-loader": "3.2.1",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.1",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0"
  }
}
