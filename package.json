{
  "name": "playground",
  "version": "2.36.0",
  "description": "Project to help with daily tasks and other projects",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "server": "nodemon server/index.js",
    "work": "npm run server & npm run start",
    "prod": "node server/index.js > server.log 2>&1",
    "build": "webpack --config config/webpack.prod.js",
    "svg": "node -r esm svgCreator/script && npm run lint",
    "lint": "eslint --fix --ext .js ./src",
    "eslint": "eslint --ext .js src/ -f html -o coverage/eslint-report.html",
    "t": "jest --env=jsdom --watch",
    "test": "jest --env=jsdom --coverage > test.log 2>&1",
    "check": "npm run eslint && npm run test && npm run build",
    "u": "rm -r node_modules/*",
    "q": "node -r esm experiment/main.js",
    "tsc": "npx tsc -p tsconfig.json",
    "tsc:watch": "npx tsc --watch -p tsconfig.json",
    "qt": "node -r esm build/q.js"
  },
  "keywords": [],
  "author": "CJV",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-react": "7.17.12",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "babel-jest": "28.1.1",
    "babel-loader": "8.2.5",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "2.0.7",
    "css-loader": "6.7.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-webpack-plugin": "3.1.1",
    "esm": "^3.2.25",
    "file-loader": "6.2.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "redux-logger": "3.0.6",
    "style-loader": "3.3.1",
    "styled-components": "5.3.5",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.9.2"
  },
  "dependencies": {
    "axios": "0.27.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "typescript": "^4.7.3"
  }
}
